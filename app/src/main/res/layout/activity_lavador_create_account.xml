<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background_create_acc"
    tools:context=".LavadorCreateAccount">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView2"
        android:layout_width="88dp"
        android:layout_height="48dp"
        android:layout_marginTop="38dp"
        app:layout_constraintBottom_toTopOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.515"
        app:srcCompat="@drawable/logo" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_text_createLavador_password"
        android:layout_width="314dp"
        android:layout_height="50dp"
        android:layout_marginTop="36dp"
        android:background="@drawable/layout_edit_text_input"
        android:hint="@string/senha"
        android:paddingBottom="0sp"
        android:paddingEnd="0sp"
        android:paddingLeft="20sp"
        android:paddingRight="0sp"
        android:paddingStart="20sp"
        android:paddingTop="0sp"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_createLavador_email"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_email"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_createLavador_email" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="264dp"
        android:layout_marginRight="264dp"
        android:layout_marginTop="216dp"
        android:text="@string/nameLavador"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_text_createLavador_name"
        android:layout_width="314dp"
        android:layout_height="49dp"
        android:layout_marginTop="56dp"
        android:background="@drawable/layout_edit_text_input"
        android:hint="@string/nameLavador"
        android:paddingBottom="0sp"
        android:paddingEnd="0sp"
        android:paddingLeft="20sp"
        android:paddingRight="0sp"
        android:paddingStart="20sp"
        android:paddingTop="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_createLavador_usuario" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="7dp"
        android:text="@string/emailLogin"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_createLavador_name"
        app:layout_constraintHorizontal_bias="0.035"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_createLavador_name" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="56dp"
        android:layout_marginRight="56dp"
        android:layout_marginTop="124dp"
        android:text="@string/usuario"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintHorizontal_bias="0.222"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/btn_createWasherAdress"
        android:layout_width="174dp"
        android:layout_height="60dp"
        android:layout_marginTop="12dp"
        android:background="@drawable/layout_button_login_activity"
        android:text="@string/buttonCreate"
        android:textColor="#FFFFFF"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_createLavador_password"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_password"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_createLavador_password" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_text_createLavador_email"
        android:layout_width="314dp"
        android:layout_height="53dp"
        android:layout_marginTop="32dp"
        android:background="@drawable/layout_edit_text_input"
        android:hint="@string/emailLogin"
        android:paddingBottom="0sp"
        android:paddingEnd="0sp"
        android:paddingLeft="20sp"
        android:paddingRight="0sp"
        android:paddingStart="20sp"
        android:paddingTop="0sp"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_usuario"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_createLavador_name" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="19dp"
        android:layout_marginTop="12dp"
        android:text="@string/senha"
        app:layout_constraintEnd_toEndOf="@+id/edit_text_createLavador_email"
        app:layout_constraintHorizontal_bias="0.036"
        app:layout_constraintStart_toStartOf="@+id/edit_text_createLavador_password"
        app:layout_constraintTop_toBottomOf="@+id/edit_text_createLavador_email" />

    <androidx.appcompat.widget.AppCompatEditText
        android:id="@+id/edit_text_createLavador_usuario"
        android:layout_width="314dp"
        android:layout_height="49dp"
        android:layout_marginTop="152dp"
        android:background="@drawable/layout_edit_text_input"
        android:hint="@string/usuario"
        android:paddingBottom="0sp"
        android:paddingEnd="0sp"
        android:paddingLeft="20sp"
        android:paddingRight="0sp"
        android:paddingStart="20sp"
        android:paddingTop="0sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>