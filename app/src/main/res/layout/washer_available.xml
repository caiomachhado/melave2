<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="145dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardElevation="8dp"
    app:cardCornerRadius="8dp"
    android:layout_margin="16dp">

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginVertical="6dp"
        android:layout_marginLeft="16dp"
        android:orientation="vertical"/>

    <androidx.appcompat.widget.LinearLayoutCompat
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical"
        android:orientation="horizontal"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView5"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="13dp"
        tools:srcCompat="@tools:sample/avatars"
        android:layout_marginStart="13dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="13dp"
        android:layout_marginLeft="155dp"
        app:srcCompat="@drawable/ic_baseline_local_car_wash_24"
        android:layout_marginStart="155dp" />

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginLeft="155dp"
        app:srcCompat="@drawable/ic_baseline_location_on_24"
        android:layout_marginStart="155dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/washer_name"
        android:layout_width="wrap_content"
        android:layout_marginTop="15dp"
        android:layout_marginLeft="185dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginStart="185dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/washer_adress"
        android:layout_width="wrap_content"
        android:layout_marginTop="47dp"
        android:layout_marginLeft="185dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginStart="185dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="155dp"
        android:text="@string/complete_wash"
        android:textStyle="bold"
        android:layout_marginTop="79dp"
        android:layout_marginStart="155dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/priceCompleteWash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="155dp"
        android:text="R$"
        android:layout_marginTop="79dp"
        android:layout_marginStart="290dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_marginTop="111dp"
        android:layout_marginLeft="155dp"
        android:text="@string/medium_wash"
        android:textStyle="bold"
        android:layout_height="wrap_content"
        android:layout_marginStart="155dp" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/priceMediumWash"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="155dp"
        android:text="R$"
        android:layout_marginTop="111dp"
        android:layout_marginStart="290dp" />


</androidx.cardview.widget.CardView>